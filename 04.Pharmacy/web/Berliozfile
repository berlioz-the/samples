---
kind: service
cluster: pharm
name: web

provides:
    client:
        port: 3000
        protocol: http
        load-balance: true

consumes:
    - service: registry_app
      endpoint: client
    - service: work_app
      endpoint: client
    - service: dash_app
      endpoint: client

scale:
    min: 1

resources:
    memory:
        min: 100
