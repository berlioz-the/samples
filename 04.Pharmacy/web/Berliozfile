---
kind: service
cluster: pharm
name: web

provides:
    client:
        port: 3000
        protocol: http
        load-balance: true

consumes:
    - service: inventory
      endpoint: client
    - service: clerk
      endpoint: client
    - service: dashboard
      endpoint: client

scale:
    min: 1

resources:
    memory:
        min: 100
